{
    "name": "thecodingmachine/tdbm-fluid-schema-builder",
    "description": "Build and modify your database schema used by TDBM using a fluid syntax.",
    "license": "MIT",
    "authors": [
        {
            "name": "David NÃ©grier",
            "email": "d.negrier@thecodingmachine.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "thecodingmachine/dbal-fluid-schema-builder": "^1.6",
        "doctrine/dbal": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5.6",
        "satooshi/php-coveralls": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "TheCodingMachine\\FluidSchema\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TheCodingMachine\\FluidSchema\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
